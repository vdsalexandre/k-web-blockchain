<div class="col s6">
    <form method="post" action="wallet">
        <div class="row">
            <div class="col s12">
                <div class="center-align">
                    <button id="create-wallet-submit-button"
                            class="btn waves-effect waves-light tooltipped"
                            style="visibility: hidden;"
                            data-position="right"
                            data-tooltip="You don't have a wallet yet, create a new one"
                            onclick="handlePreloader('show')"
                            type="submit">Create
                        <i class="material-icons right">account_balance_wallet</i>
                    </button>
                </div>
            </div>
        </div>
    </form>
</div>